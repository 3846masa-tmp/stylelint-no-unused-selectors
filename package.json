{
  "name": "stylelint-no-unused-selectors",
  "version": "1.0.0",
  "description": "Disallow unused selectors",
  "main": "dist/index.js",
  "repository": "https://github.com/nodaguti/stylelint-no-unused-selectors",
  "author": "nodaguti",
  "license": "MIT",
  "keywords": [],
  "files": [
    "README.md",
    "package.json",
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.json --outDir dist",
    "lint": "tsc --project ./tsconfig.json --noEmit && eslint --cache 'src/**/*.{ts,js}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@typescript-eslint/parser": "^1.6.0",
    "array.prototype.flatmap": "^1.2.1",
    "espree": "^5.0.1",
    "jsdom": "^14.0.0",
    "option-t": "^19.2.0",
    "postcss-resolve-nested-selector": "^0.1.1",
    "string-template": "^1.0.0"
  },
  "peerDependencies": {
    "stylelint": "^9.10.1"
  },
  "devDependencies": {
    "@types/array.prototype.flatmap": "^1.2.0",
    "@types/jest": "^24.0.11",
    "@types/jsdom": "^12.2.3",
    "@types/node": "^11.13.0",
    "@types/string-template": "^1.0.2",
    "@types/stylelint": "^9.10.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "stylelint": "^9.10.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.2"
  }
}
